gzip
ext .html

jwt {
	path /
	redirect /login?backTo={rewrite_uri}
	allow sub bob
	passthrough /
}

login {
	simple bob=secret
}

proxy /robo/ http://robo:8081 {
	header_upstream Host {host}
	header_upstream X-Real-IP {remote}
	header_upstream X-Forwarded-For {remote}
	without /robo/
	websocket
}

errors {
	500 error5xx.html
	502 error5xx.html
}